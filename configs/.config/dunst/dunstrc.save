[global]
frame_color = "#89b4fa"
separator_color= frame
highlight = "#89b4fa"
corner_radius = 5
font = * {
    font-family: "JetBrainsMono Nerd Font";
    font-weight: bold;
        min-height: 0;
    /* set font-size to 100% if font scaling is set to 1.00 using nwg-look */
    font-size: 97%;
    font-feature-settings: '"zero", "ss01", "ss02", "ss03", "ss04", "ss05", "cv31"';
}

[urgency_low]
background = "#1e1e2e"
foreground = "#cdd6f4"

[urgency_normal]
background = "#1e1e2e"
foreground = "#cdd6f4"

[urgency_critical]
background = "#1e1e2e"
foreground = "#cdd6f4"
frame_color = "#fab387"

[discord]
appname = "Discord"
urgency = normal
frame_color = "#747da6" 
